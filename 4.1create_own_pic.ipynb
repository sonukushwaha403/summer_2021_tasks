    "\n",
    "font = cv.FONT_HERSHEY_SIMPLEX\n",
    "img = np.zeros((200, 500, 3), np.uint8)\n",
    "cv.putText(img,'OpenCV', p0, font, 4, RED, 2, cv.LINE_AA)\n",
    "cv.imshow('OWN IMAGE', img)\n",
    "\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()"
